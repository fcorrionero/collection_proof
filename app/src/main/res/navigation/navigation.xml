<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/deviceData">

    <fragment
        android:id="@+id/deviceData"
        android:name="com.fcorrionero.myapplication.DeviceData"
        android:label="fragment_device_data"
        tools:layout="@layout/fragment_device_data" >
        <action
            android:id="@+id/action_deviceData_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.fcorrionero.myapplication.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>